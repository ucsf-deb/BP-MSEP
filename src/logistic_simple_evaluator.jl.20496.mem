        - "Evaluate zsimp, a potential analogue of zBP"
        - function zsimp(ev::LogisticSimpleEvaluator, wa::WorkArea) where {WorkArea}
490798704     f(z) = ev.f(z, wa)
    60928     wa.objective = justZ
  1453008     zsimp = ev.integrator(f, segbuf=wa.segs)
    58496     wa.objective = just1
  1439472     den1 = ev.integrator(f, segbuf=wa.segs)
   510336     return zsimp/den1
        - end
